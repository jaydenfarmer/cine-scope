{
  "version": 2,
  "name": "movie-match",
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist/movie-match/browser",
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
